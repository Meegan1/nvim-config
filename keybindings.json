[
  {
    "key": "ctrl+u",
    "command": "runCommands",
    "args": {
      "commands": [
        {
          "command": "editorScroll",
          "args": {
            "to": "up",
            "by": "wrappedLine",
            "value": 20
          }
        },
        {
          "command": "cursorMove",
          "args": {
            "to": "up",
            "by": "wrappedLine",
            "value": 20
          }
        }
      ]
    }
  },
  {
    "key": "ctrl+d",
    "command": "runCommands",
    "args": {
      "commands": [
        {
          "command": "editorScroll",
          "args": {
            "to": "down",
            "by": "wrappedLine",
            "value": 20
          }
        },
        {
          "command": "cursorMove",
          "args": {
            "to": "down",
            "by": "wrappedLine",
            "value": 20
          }
        }
      ]
    }
  },
  {
    "key": "alt+h",
    "command": "vscode-neovim.send",
    "args": "<A-h>",
    "when": "editorTextFocus && neovim.init"
  },
  {
    "key": "alt+j",
    "command": "vscode-neovim.send",
    "args": "<A-j>",
    "when": "editorTextFocus && neovim.init"
  },
  {
    "key": "alt+k",
    "command": "vscode-neovim.send",
    "args": "<A-k>",
    "when": "editorTextFocus && neovim.init"
  },
  {
    "key": "alt+l",
    "command": "vscode-neovim.send",
    "args": "<A-l>",
    "when": "editorTextFocus && neovim.init"
  }
