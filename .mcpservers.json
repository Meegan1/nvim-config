{
  "mcpServers": {
    "context7": {
      "args": ["-y", "@upstash/context7-mcp"],
      "command": "npx",
      "env": {
        "DEFAULT_MINIMUM_TOKENS": "10000"
      }
    },
    "fetch": {
      "args": ["mcp-server-fetch"],
      "command": "uvx",
      "disabled": false
    },
    "filesystem": {
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "."],
      "command": "npx"
    }
  },
  "nativeMCPServers": []
}
